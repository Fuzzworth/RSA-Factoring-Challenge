#!/usr/bin/python3
from sys import argv
import math

def lowest_prime_factor(n):
    if n % 2 == 0:
        return 2
    sqrt_n = math.isqrt(n) + 1
    for i in range(3, sqrt_n, 2):
        if n % i == 0:
            return i
    return n

if len(argv) != 2:
    print("{} <file>".format(argv[0]))
with open(argv[1]) as factor_file:
    for line in factor_file:
        line = int(line)
        factor = lowest_prime_factor(line)
        print("{:d}={:d}*{:d}".format(line, int(line / factor), int(factor)))
